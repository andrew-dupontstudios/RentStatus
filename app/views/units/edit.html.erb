<%= form_for :unit, url: property_path(@unit), method: :patch do |f| %>
	<div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :rent %><br>
    <%= f.number_field :rent %>
  </div>
  
  <div class="field">
    <%= f.label :tenant0_first_name %><br>
    <%= f.text_field :tenant0_first_name %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
  
<% end %>
